@model SMS.Models.StudentModel

@{
    ViewBag.Title = "Get_std";
    Layout = "~/Views/Shared/_Layout_logout.cshtml";
}
<h2>Search Form</h2>

<div>
    <h4>Student Information</h4>
    <hr />

    @using (Html.BeginForm())
    {
        <div class="form-group">
            @Html.LabelFor(model => model.std_id, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.std_id, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.std_id, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Submit" class="btn btn-default" />
            </div>
        </div>
    }

    <hr />

    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.std_name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.std_name)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.std_pass)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.std_pass)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.std_f_name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.std_f_name)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.std_gender)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.std_gender)
        </dd>

      
        <dt>
            @Html.DisplayNameFor(model => model.std_age)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.std_age)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.std_Dob)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.std_Dob)
        </dd>


        <dt>
            @Html.DisplayNameFor(model => model.std_contact)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.std_contact)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.std_f_contact)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.std_f_contact)
        </dd>


        <dt>
            @Html.DisplayNameFor(model => model.std_address)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.std_address)
        </dd>

    </dl>

</div>

